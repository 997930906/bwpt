<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="bwpt.ssp.domain.Ssp" table="ssp">

	<id name="id" column="ssp_id">
		<generator class="native"></generator>
	</id>
	<property name="path" column="ssp_path"></property>
	<property name="state" column="ssp_state"></property>
</class>
</hibernate-mapping>